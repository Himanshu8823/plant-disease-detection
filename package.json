{
  "name": "plant-disease-detector-pro",
  "version": "2.0.0",
  "description": "A comprehensive plant disease detection application with AI-powered analysis",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "setup": "node setup.js",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "reset": "npm run clean && npm run install:all"
  },
  "keywords": [
    "plant",
    "disease",
    "detection",
    "ai",
    "machine-learning",
    "react-native",
    "nodejs",
    "firebase",
    "gemini",
    "agriculture"
  ],
  "author": "Plant Disease Detector Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/plant-disease-detector-pro.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/plant-disease-detector-pro/issues"
  },
  "homepage": "https://github.com/your-username/plant-disease-detector-pro#readme"
}